@model List<AdminPrototypTest.Models.BookingsModel>
@{
    ViewBag.Title = "Index";
}

<h2>Boknignsadmins gränssnitt</h2>

<table class="table table-striped">
    <tr>
        <th>Boknings id</th>
        <th>Event id</th>
        <th>Host id</th>
        <th>Start datum och tid</th>
        <th>Pris</th>
        <th>Användar id</th>
        <th>Användartyp</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@Html.DisplayFor(m => item.Booking_Id)</td>
            <td>@Html.DisplayFor(m => item.Event_Id)</td>
            <td>@Html.DisplayFor(m => item.Event_Host_Id)</td>
            <td>@Html.DisplayFor(m => item.Event_Start_Date_Time)</td>
            <td>@Html.DisplayFor(m => item.Booking_Price)</td>
            <td>@Html.DisplayFor(m => item.User_Id)</td>
            <td>@Html.DisplayFor(m => item.User_Type)</td>
            <td>@Html.ActionLink("Detaljer", "Detaljer", new { id = item.Booking_Id})</td>
        </tr>
    }

</table>

